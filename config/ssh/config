Host github.com-personal
	HostName github.com
	User git
	IdentityFile ~/.ssh/personal
    AddKeysToAgent yes

Host github.com-work
	HostName github.com
	User git
	IdentityFile ~/.ssh/work
    AddKeysToAgent yes

Host gitlab.com
    HostName gitlab.com
    User git
    IdentityFile ~/.ssh/personal
    AddKeysToAgent yes
